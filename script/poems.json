[
    {
        "poem_num": 2,
        "text": [
            "白日依山尽",
            "黄河入海流",
            "欲穷千里目",
            "更上一层楼"
        ],
        "author": "王之涣",
        "title": "登鹳雀楼"
    },
    {
        "poem_num": 3,
        "text": [
            "春眠不觉晓",
            "处处闻啼鸟",
            "夜来风雨声",
            "花落知多少"
        ],
        "author": "孟浩然",
        "title": "春晓"
    },
    {
        "poem_num": 4,
        "text": [
            "红豆生南国",
            "春来发几枝",
            "愿君多采撷",
            "此物最相思"
        ],
        "author": "王维",
        "title": "相思"
    },
    {
        "poem_num": 5,
        "text": [
            "慈母手中线",
            "游子身上衣",
            "临行密密缝",
            "意恐迟迟归"
        ],
        "author": "孟郊",
        "title": "游子吟"
    },
    {
        "poem_num": 6,
        "text": [
            "空山不见人",
            "但闻人语响",
            "返景入深林",
            "复照青苔上"
        ],
        "author": "王维",
        "title": "鹿柴"
    },
    {
        "poem_num": 7,
        "text": [
            "锄禾日当午",
            "汗滴禾下土",
            "谁知盘中餐",
            "粒粒皆辛苦"
        ],
        "author": "李绅",
        "title": "悯农"
    },
    {
        "poem_num": 8,
        "text": [
            "离离原上草",
            "一岁一枯荣",
            "野火烧不尽",
            "春风吹又生"
        ],
        "author": "白居易",
        "title": "赋得古原草送别"
    },
    {
        "poem_num": 9,
        "text": [
            "向晚意不适",
            "驱车登古原",
            "夕阳无限好",
            "只是近黄昏"
        ],
        "author": "李商隐",
        "title": "登乐游原"
    },
    {
        "poem_num": 10,
        "text": [
            "千山鸟飞绝",
            "万径人踪灭",
            "孤舟蓑笠翁",
            "独钓寒江雪"
        ],
        "author": "柳宗元",
        "title": "江雪"
    },
    {
        "poem_num": 11,
        "text": [
            "独怜幽草涧边生",
            "上有黄鹂深树鸣",
            "春潮带雨晚来急",
            "野渡无人舟自横"
        ],
        "author": "韦应物",
        "title": "滁州西涧"
    },
    {
        "poem_num": 12,
        "text": [
            "朝辞白帝彩云间",
            "千里江陵一日还",
            "两岸猿声啼不住",
            "轻舟已过万重山"
        ],
        "author": "李白",
        "title": "早发白帝城"
    },
    {
        "poem_num": 13,
        "text": [
            "两个黄鹂鸣翠柳",
            "一行白鹭上青天",
            "窗含西岭千秋雪",
            "门泊东吴万里船"
        ],
        "author": "杜甫",
        "title": "绝句"
    },
    {
        "poem_num": 14,
        "text": [
            "好雨知时节",
            "当春乃发生",
            "随风潜入夜",
            "润物细无声"
        ],
        "author": "杜甫",
        "title": "春夜喜雨"
    },
    {
        "poem_num": 15,
        "text": [
            "月落乌啼霜满天",
            "江枫渔火对愁眠",
            "姑苏城外寒山寺",
            "夜半钟声到客船"
        ],
        "author": "张继",
        "title": "枫桥夜泊"
    },
    {
        "poem_num": 16,
        "text": [
            "秦时明月汉时关",
            "万里长征人未还",
            "但使龙城飞将在",
            "不教胡马度阴山"
        ],
        "author": "王昌龄",
        "title": "出塞"
    },
    {
        "poem_num": 17,
        "text": [
            "远上寒山石径斜",
            "白云深处有人家",
            "停车坐爱枫林晚",
            "霜叶红于二月花"
        ],
        "author": "杜牧",
        "title": "山行"
    },
    {
        "poem_num": 18,
        "text": [
            "清明时节雨纷纷",
            "路上行人欲断魂",
            "借问酒家何处有",
            "牧童遥指杏花村"
        ],
        "author": "杜牧",
        "title": "清明"
    },
    {
        "poem_num": 19,
        "text": [
            "少小离家老大回",
            "乡音无改鬓毛衰",
            "儿童相见不相识",
            "笑问客从何处来"
        ],
        "author": "贺知章",
        "title": "回乡偶书"
    },
    {
        "poem_num": 20,
        "text": [
            "君自故乡来",
            "应知故乡事",
            "来日绮窗前",
            "寒梅著花未"
        ],
        "author": "王维",
        "title": "杂诗"
    },
    {
        "poem_num": 21,
        "text": [
            "海上生明月",
            "天涯共此时",
            "情人怨遥夜",
            "竟夕起相思"
        ],
        "author": "张九龄",
        "title": "望月怀远"
    },
    {
        "poem_num": 22,
        "text": [
            "移舟泊烟渚",
            "日暮客愁新",
            "野旷天低树",
            "江清月近人"
        ],
        "author": "孟浩然",
        "title": "宿建德江"
    },
    {
        "poem_num": 23,
        "text": [
            "众鸟高飞尽",
            "孤云独去闲",
            "相看两不厌",
            "只有敬亭山"
        ],
        "author": "李白",
        "title": "独坐敬亭山"
    },
    {
        "poem_num": 24,
        "text": [
            "人闲桂花落",
            "夜静春山空",
            "月出惊山鸟",
            "时鸣春涧中"
        ],
        "author": "王维",
        "title": "鸟鸣涧"
    },
    {
        "poem_num": 25,
        "text": [
            "白日登山望烽火",
            "黄昏饮马傍交河",
            "行人刁斗风沙暗",
            "公主琵琶幽怨多"
        ],
        "author": "李颀",
        "title": "古从军行"
    },
    {
        "poem_num": 26,
        "text": [
            "葡萄美酒夜光杯",
            "欲饮琵琶马上催",
            "醉卧沙场君莫笑",
            "古来征战几人回"
        ],
        "author": "王翰",
        "title": "凉州词"
    },
    {
        "poem_num": 27,
        "text": [
            "故人西辞黄鹤楼",
            "烟花三月下扬州",
            "孤帆远影碧空尽",
            "唯见长江天际流"
        ],
        "author": "李白",
        "title": "黄鹤楼送孟浩然之广陵"
    },
    {
        "poem_num": 28,
        "text": [
            "月黑雁飞高",
            "单于夜遁逃",
            "欲将轻骑逐",
            "大雪满弓刀"
        ],
        "author": "卢纶",
        "title": "塞下曲"
    },
    {
        "poem_num": 29,
        "text": [
            "曾经沧海难为水",
            "除却巫山不是云",
            "取次花丛懒回顾",
            "半缘修道半缘君"
        ],
        "author": "元稹",
        "title": "离思五首·其四"
    },
    {
        "poem_num": 30,
        "text": [
            "松下问童子",
            "言师采药去",
            "只在此山中",
            "云深不知处"
        ],
        "author": "贾岛",
        "title": "寻隐者不遇"
    },
    {
        "poem_num": 31,
        "text": [
            "岭外音书断",
            "经冬复历春",
            "近乡情更怯",
            "不敢问来人"
        ],
        "author": "宋之问",
        "title": "渡汉江"
    },
    {
        "poem_num": 32,
        "text": [
            "山中相送罢",
            "日暮掩柴扉",
            "春草年年绿",
            "王孙归不归"
        ],
        "author": "王维",
        "title": "送别"
    },
    {
        "poem_num": 33,
        "text": [
            "功盖三分国",
            "名成八阵图",
            "江流石不转",
            "遗恨失吞吴"
        ],
        "author": "杜甫",
        "title": "八阵图"
    },
    {
        "poem_num": 34,
        "text": [
            "终南阴岭秀",
            "积雪浮云端",
            "林表明霁色",
            "城中增暮寒"
        ],
        "author": "祖咏",
        "title": "终南望余雪"
    },
    {
        "poem_num": 35,
        "text": [
            "泠泠七弦上",
            "静听松风寒",
            "古调虽自爱",
            "今人多不弹"
        ],
        "author": "刘长卿",
        "title": "弹琴"
    },
    {
        "poem_num": 36,
        "text": [
            "三日入厨下",
            "洗手作羹汤",
            "未谙姑食性",
            "先遣小姑尝"
        ],
        "author": "王建",
        "title": "新嫁娘词"
    },
    {
        "poem_num": 37,
        "text": [
            "北斗七星高",
            "哥舒夜带刀",
            "至今窥牧马",
            "不敢过临洮"
        ],
        "author": "西鄙人",
        "title": "哥舒歌"
    },
    {
        "poem_num": 38,
        "text": [
            "打起黄莺儿",
            "莫教枝上啼",
            "啼时惊妾梦",
            "不得到辽西"
        ],
        "author": "金昌绪",
        "title": "春怨"
    },
    {
        "poem_num": 39,
        "text": [
            "寥落古行宫",
            "宫花寂寞红",
            "白头宫女在",
            "闲坐说玄宗"
        ],
        "author": "元稹",
        "title": "行宫"
    },
    {
        "poem_num": 40,
        "text": [
            "绿蚁新醅酒",
            "红泥小火炉",
            "晚来天欲雪",
            "能饮一杯无"
        ],
        "author": "白居易",
        "title": "问刘十九"
    },
    {
        "poem_num": 41,
        "text": [
            "昨夜裙带解",
            "今朝蟢子飞",
            "铅华不可弃",
            "莫是藁砧归"
        ],
        "author": "权德舆",
        "title": "玉台体"
    },
    {
        "poem_num": 42,
        "text": [
            "千山万水不曾行",
            "魂梦欲教何处觅",
            "除却巫山不是云",
            "取次花丛懒回顾"
        ],
        "author": "元稹",
        "title": "离思"
    }
]